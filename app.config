<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
    </startup>
<system.serviceModel>
	<behaviors>
		<serviceBehaviors>
			<behavior name="">
				<serviceMetadata httpsGetEnabled="true" httpGetEnabled="true"/>
				<serviceDebug includeExceptionDetailInFaults="false"/>
			</behavior>
		</serviceBehaviors>
	</behaviors>
	<services>
		<service name="WCF_Service.ServiceChat">
			<endpoint address="" binding="basicHttpBinding" contract="WCF_Service.IServiceChat">
				<identity>
					<dns value="localhost"/>
				</identity>
			</endpoint>
		<endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
			<host>
				<baseAddresses>
					<add baseAddress="http://localhost:8733/Design_Time_Addresses/WCF_Service/ServiceChat/" />
				</baseAddresses>
			</host>
		</service>
	</services>
</system.serviceModel>
</configuration>